//count sum of first 30 positive odd integers

public class Main {
    public static void main(String[] args) {
        int count=1;
        int sum=0;
        int num=1;
        do{
            if(num%2!=0 && count<=30){
                sum+=num;
                count+=1;

            }
            num+=1;
        }while(num>0);
        System.out.println("sum of first 30 positive odd integers is: "+sum);
    }
}
